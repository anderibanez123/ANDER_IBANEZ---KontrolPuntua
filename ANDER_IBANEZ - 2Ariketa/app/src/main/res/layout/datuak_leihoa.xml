<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".datuak_leihoa">

    <TextView
        android:id="@+id/tlfTextView"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/izenaTextView"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="20dp"
        android:text="Telefonoa:"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/izenaTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="20dp"
        android:text="Izena:"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/korreoaTextView"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/dataTextView"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="20dp"
        android:text="Korreoa:"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/abizenaTextView"
        android:layout_width="104dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/korreoaTextView"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="20dp"
        android:text="Abizena:"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/dataTextView"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/tlfTextView"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="20dp"
        android:text="Data:"
        android:textSize="20dp" />

</RelativeLayout>
